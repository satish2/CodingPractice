#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <assert.h>
/* Head ends here */
int check_sim(char *, char *);
long long int stringSimilarity(char a[]) {
    int sum = 0;
    int len = strlen(a);
    int i;
    for(i =0; i< len; i++){
        sum +=strspn(a, a+i);
    }
    return sum;
}

int main() {
    int t, i;
    long long int res;
    scanf("%d",&t);
    char a[100001];
    for (i=0;i<t;i++) {
        scanf("%s",a);
        res=stringSimilarity(a);
        printf("%lld\n",res);  
    }

    return 0;
}
